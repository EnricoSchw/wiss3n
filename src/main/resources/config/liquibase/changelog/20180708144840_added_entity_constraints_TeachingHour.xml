    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity TeachingHour.
    -->
    <changeSet id="20180708144840-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="teaching_subject_id"
                                 baseTableName="teaching_hour"
